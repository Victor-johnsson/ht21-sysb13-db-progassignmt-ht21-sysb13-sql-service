<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="parentContainer" prefHeight="657.0" prefWidth="1011.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sqlservice.AdminViewController">
   <children>
      <AnchorPane layoutX="14.0" layoutY="12.0">
         <children>
            <AnchorPane prefHeight="630.0" prefWidth="985.0">
               <children>
                  <MenuButton layoutX="14.0" layoutY="14.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="81.0" text="Go To">
                    <items>
                      <MenuItem mnemonicParsing="false" onAction="#loadCourseScene" text="Courses" />
                      <MenuItem mnemonicParsing="false" onAction="#loadStudentScene" text="Student" />
                    </items>
                  </MenuButton>
                  <TableView fx:id="courseTableView" layoutX="8.0" layoutY="65.0" prefHeight="200.0" prefWidth="266.0" />
                  <TableView fx:id="studentTableView" layoutX="536.0" layoutY="65.0" prefHeight="200.0" prefWidth="368.0" />
                  <Button fx:id="showStudentsOnCourse" layoutX="2.0" layoutY="290.0" mnemonicParsing="false" onAction="#onShowStudentsOnCourse" prefHeight="33.0" prefWidth="155.0" text="Show Students On Course" />
                  <TextField fx:id="searchStudentTextField" layoutX="574.0" layoutY="265.0" prefHeight="25.0" prefWidth="273.0" promptText="Search Student" />
                  <TextField fx:id="searchCourseTextField" layoutX="30.0" layoutY="265.0" prefHeight="25.0" prefWidth="214.0" promptText="Search Course" />
                  <Button fx:id="setGradeButton" layoutX="311.0" layoutY="232.0" mnemonicParsing="false" onAction="#setGrade" text="Set Grade/Reg of completed Course" />
                  <Button fx:id="showCoursesStudied" layoutX="639.0" layoutY="299.0" mnemonicParsing="false" onAction="#onShowStudiedCourses" text="Show Active Courses" />
                  <Button fx:id="removeStudentFromCourseButton" layoutX="321.0" layoutY="140.0" mnemonicParsing="false" onAction="#removeStudentFromCourse" text="Remove Student From Course---&gt;" />
                  <TextArea fx:id="feedbackTextArea" focusTraversable="false" layoutX="315.0" layoutY="394.0" prefHeight="200.0" prefWidth="200.0" promptText="Feedback" />
                  <Button fx:id="showCompletedCourses" layoutX="651.0" layoutY="338.0" mnemonicParsing="false" onAction="#onShowCompletedCourses" text="Completed Courses" />
                  <Button fx:id="resetButton" layoutX="364.0" layoutY="351.0" mnemonicParsing="false" onAction="#onResetButton" text="ResetButton" />
                  <Button fx:id="showCourseStatistics" layoutX="166.0" layoutY="290.0" mnemonicParsing="false" onAction="#onCourseStatistics" prefHeight="33.0" prefWidth="140.0" text="Show Course Statistics" />
                  <Button fx:id="addStudentOnCourseButton" layoutX="338.0" layoutY="78.0" mnemonicParsing="false" onAction="#addStudentOnCourse" text="&lt;---Add Student To Course" />
                  <ComboBox fx:id="gradesComboBox" layoutX="374.0" layoutY="207.0" opacity="0.8" prefHeight="25.0" prefWidth="81.0" promptText="Grades" />
                  <Button fx:id="throughput" layoutX="30.0" layoutY="326.0" mnemonicParsing="false" onAction="#onThroughputButton" text="Show Courses With Highest Throughput" />
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
